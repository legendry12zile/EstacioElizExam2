<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"><![CDATA[
    tools:context=".Main2Activity">







    ]]>

    <TextView
        android:id="@+id/textView15"
        android:layout_width="344dp"
        android:layout_height="104dp"
        android:layout_alignBottom="@+id/textView2"
        android:layout_centerHorizontal="true"
        android:background="@android:color/darker_gray" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="344dp"
        android:layout_height="138dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="348dp"
        android:background="@android:color/darker_gray" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="344dp"
        android:layout_height="104dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="111dp"
        android:background="@android:color/darker_gray" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:background="@color/colorPrimaryDark"
        android:padding="10dp"
        android:text="Card Activity"
        android:textAppearance="@style/TextStyle.Serif"
        android:textColor="@color/colorPrimary"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="109dp"
        android:layout_height="28dp"
        android:layout_above="@+id/textView9"
        android:layout_alignStart="@+id/textView9"
        android:layout_marginBottom="-148dp"
        android:text="Spongebob"
        android:textAppearance="@style/TextStyle.Serif"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_alignBottom="@+id/textView8"
        android:layout_toEndOf="@+id/textView8"
        android:text="Cook" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="305dp"
        android:layout_height="57dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="148dp"
        android:text="SpongeBob SquarePants is an American animated television series created by marine biologist and animator Stephen Hillenburg for Nickelodeon."
        android:textAppearance="@style/TextDescrip.Monospace" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="26dp"
        android:layout_above="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:text="Patrick"
        android:textAppearance="@style/TextStyle.Serif"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_alignBottom="@+id/textView7"
        android:layout_toEndOf="@+id/textView7"
        android:text="Best friend" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="270dp"
        android:layout_height="51dp"
        android:layout_centerInParent="true"
        android:text="Patrick Star is a fictional character in the American animated television series SpongeBob SquarePants."
        android:textAppearance="@style/TextDescrip.Monospace" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_above="@+id/textView2"
        android:layout_alignStart="@+id/textView7"
        android:text="Sandy"
        android:textAppearance="@style/TextStyle.Serif"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_alignBottom="@+id/textView"
        android:layout_alignEnd="@+id/textView10"
        android:text="Martial Artist" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="266dp"
        android:layout_height="64dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="135dp"
        android:text=" She is a squirrel who wears a diving suit and lives underwater."
        android:textAppearance="@style/TextDescrip.Monospace" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="43dp"
        android:layout_height="52dp"
        android:layout_alignEnd="@+id/textView15"
        android:layout_alignTop="@+id/textView"
        android:visibility="visible"
        app:srcCompat="@drawable/g" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="43dp"
        android:layout_height="52dp"
        android:layout_alignEnd="@+id/textView15"
        android:layout_alignTop="@+id/textView16"
        android:adjustViewBounds="true"
        android:visibility="visible"
        app:srcCompat="@drawable/p" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="43dp"
        android:layout_height="52dp"
        android:layout_alignEnd="@+id/textView15"
        android:layout_alignTop="@+id/textView17"
        android:visibility="visible"
        app:srcCompat="@drawable/s" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView15"
        android:layout_marginBottom="39dp"
        android:onClick="onClick"
        android:text="Next" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView15"
        android:layout_alignTop="@+id/button2"
        android:onClick="onClick"
        android:text="Map" />


</RelativeLayout>